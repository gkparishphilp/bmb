!!!strict
%html
	%head
		%link( rel="shortcut icon" href="/images/favicon.ico" )
		

		= stylesheet_link_tag 'screen2.css', :media => 'screen, projection'
		= include_tiny_mce_if_needed

		-#= stylesheet_link_tag 'print.css', :media => 'print'
		-#= stylesheet_link_tag 'http://fonts.googleapis.com/css?family=Cantarell:regular,bold&subset=latin'
		
		/[if IE]
			= stylesheet_link_tag 'ie.css', :media => 'screen, projection'
	
		= stylesheet_link_tag 'facebox'
		
		= javascript_include_tag 'http://ajax.googleapis.com/ajax/libs/jquery/1.4.2/jquery.min.js?ver=1.4.2','jquery-ui.min','rails','application','facebox'
		/= javascript_include_tag :defaults
		/= javascript_include_tag 'facebox'
		
		= csrf_meta_tag
		
		%title
			= get_title

	%body
		#container
			= render :partial => 'shared/header'
			#bd
				= render :partial => 'shared/flash', :locals => { :flash => flash }
				= yield
			= render :partial => 'shared/footer'
		= render :partial => 'shared/analytics'
		= render :partial => 'shared/scripts'

